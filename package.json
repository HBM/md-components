{
  "name": "md-components",
  "version": "7.2.1",
  "description": "Material Design React components",
  "main": "lib/index.js",
  "scripts": {
    "stylelint": "stylelint src/css/*.scss src/js/**/*.scss",
    "lib": "webpack",
    "lib:watch": "webpack --watch",
    "examples": "cd examples && react-scripts start",
    "test": "standard && npm run stylelint && istanbul cover ./node_modules/mocha/bin/_mocha test/.setup.js src/js/**/test/test.js",
    "release": "npm run lib && cpx ./src/css/**/* ./lib/css"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hbm/md-components.git"
  },
  "keywords": [
    "react",
    "components",
    "material",
    "design"
  ],
  "author": "Mirco Zeiss <mirco.zeiss@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "coveralls": "^2.11.12",
    "cpx": "^1.5.0",
    "css-loader": "^0.26.1",
    "envify": "^3.4.0",
    "enzyme": "^2.3.0",
    "eslint-plugin-react": "^6.0.0",
    "glob": "^7.0.5",
    "history": "^1.12.6",
    "http-server": "^0.9.0",
    "istanbul": "^1.1.0-alpha.1",
    "jasmine-core": "^2.3.4",
    "jsdom": "^7.2.2",
    "mocha": "^3.0.2",
    "node-sass": "^3.6.0",
    "react": "^15.4.0",
    "react-addons-test-utils": "^15.3.0",
    "react-broadcast": "^0.1.2",
    "react-dom": "^15.4.0",
    "react-router": "4.0.0-alpha.6",
    "react-scripts": "^0.8.4",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "saucelabs": "^1.2.0",
    "selenium-webdriver": "^3.0.0-beta-2",
    "st": "^1.2.0",
