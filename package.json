{
  "name": "md-components",
  "version": "12.1.0",
  "description": "Material Design React components",
  "main": "lib/index.js",
  "module": "es/index.js",
  "style": "style.css",
  "scripts": {
    "stylelint": "stylelint src/css/*.scss src/js/**/*.scss",
    "build:css": "node-sass src/css/index.scss style.css",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build": "npm run build:css && npm run build:commonjs && npm run build:es",
    "examples": "cd examples && react-scripts start",
    "test": "standard && npm run stylelint && nyc mocha src/js/**/test/test.js",
    "prepublish": "npm run build && cpx ./src/css/**/* ./lib/css"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hbm/md-components.git"
  },
  "keywords": [
    "react",
    "components",
    "material",
    "design"
  ],
  "author": "Mirco Zeiss <mirco.zeiss@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.2.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "coveralls": "^2.11.12",
    "cpx": "^1.5.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.27.3",
    "enzyme": "^2.3.0",
    "eslint-plugin-react": "^6.10.3",
    "glob": "^7.0.5",
    "history": "^4.6.1",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.3.4",
    "jsdom": "^9.12.0",
    "mocha": "^3.0.2",
    "node-sass": "^4.5.0",
    "nyc": "^10.1.2",
    "react": "^15.4.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.4.0",
    "react-router-dom": "^4.0.0",
    "react-scripts": "^1.0.7",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.3",
    "saucelabs": "^1.2.0",
    "st": "^1.2.0",
    "standard": "^9.0.2",
    "style-loader": "^0.15.0",
    "stylelint": "^7.3.1",
    "stylelint-config-standard": "^16.0.0",
    "tarball-extract": "0.0.3",
    "uglifyjs": "^2.4.10"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "babel-register",
      "./test/.setup.js"
    ],
    "exclude": [
      "**/test.js",
      "test"
    ]
  },
  "dependencies": {
    "attr-accept": "^1.0.3",
    "classnames": "^2.2.4",
    "keycode": "^2.1.8",
    "prop-types": "^15.5.10",
    "react-motion": "^0.4.3"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "es/",
      "src/js/icon/index.js",
      "build/"
    ]
  }
}
